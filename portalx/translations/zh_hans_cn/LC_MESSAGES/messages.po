#: portalx/common.py:69
#, python-format
msgid "Unsupported URL scheme \"%(scheme)s\""
msgstr "不支持 %(scheme)s 协议"

#: portalx/common.py:72
#, python-format
msgid "URL <code>%(url)s</code> missing website domain name or location."
msgstr "地址 <code>%(url)s</code> 并不包含有效域名或 IP 地址。"

#: portalx/common.py:117
#, python-format
msgid "Got HTTP %(code)d while accessing <code>%(url)s</code>"
msgstr "尝试访问 <code>%(url)s</code> 时收到了 %(code)d 状态码。"

#: portalx/common.py:119
#, python-format
msgid "Unable to access <code>%(url)s</code><br/>Too many redirects."
msgstr "无法访问 <code>%(url)s</code>，因为跳转过多。"

#: portalx/common.py:121
#, python-format
msgid ""
"Unable to access <code>%(url)s</code><br/>An TLS/SSL error occured, "
"remote server may not support HTTPS."
msgstr "无法访问 <code>%(url)s</code>，因为遇到了一个 TLS/SSL 错误。远端服务器可能不支持 HTTPS."

#: portalx/common.py:123
#, python-format
msgid ""
"Unable to access <code>%(url)s</code><br/>Resource may not exist, or be "
"available to the server, or outgoing traffic at the server may be "
"disrupted."
msgstr "无法访问 <code>%(url)s</code>。这个地址可能不存在，也有可能服务器遇到了网络问题。"

#: portalx/common.py:125
#, python-format
msgid ""
"\n"
"        <pre><code>An unhandled error occured while processing this "
"request.\n"
"        Parsed URL: %(url)s\n"
"        Error name: %(errname)s</code></pre>\n"
"        "
msgstr ""
"\n"
"        <pre><code>处理请求时服务器遇到了一个无法恢复的错误。\n"
"        请求的 URL: %(url)s\n"
"        错误名: %(errname)s</code></pre>\n"
"        "

